generator client {
  provider = "prisma-client"
  // Generate the client inside the source tree so the app can import it directly
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String        @id     @default(uuid())
  username  String      
  password  String      
}
